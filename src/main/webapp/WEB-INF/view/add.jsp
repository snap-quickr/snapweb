          <li class="divider"></li>
        </ul>
        <div class="right_menu_corner"></div>
      </div>
      <!-- end of menu tab -->
      <div class="center_content">
        <div class="center_add_content">
          <center>
          <h4>Add your product</h4>
          <form action="#">
              <div class="add_labels">Select place</div>
              <div class="add_input">
                <select name="locationId">
                  <option value="0">&nbsp; Choose</option>
                  <option value="1">Gurgaon</option>
                  <option value="2">Delhi</option>
                  <option value="3">Mumbai</option>
                  <option value="4">Banglore</option>
                  <option value="5">Hydrabad</option>
                  <option value="6">Jaipur</option>
                  <option value="7">Chennai</option>
                  <option value="8">Kolkata</option>
                  <option value="9">Amritsar</option>
                  <option value="10">Shimla</option>
                </select>
              </div>
              <div class="add_labels">Select place</div>
              <div class="add_input">
                <select name="categoryId">
                  <option value="0">&nbsp; &nbsp; &nbsp;e.g., Cars &amp Bikes</option>
                  <option value="1">Cars & Bikes</option>
                  <option value="2">Mobiles & Tablets</option>
                  <option value="3">Electronics & Appliances</option>
                  <option value="4">Real Estate</option>
                  <option value="5">Home & LifeStyle</option>
                </select>
              </div>
              <div class="add_labels">Name of Product</div><input type="text" name="productName" class="add_input" placeholder=" Moto G3">
              <div class="add_labels">Price of Product</div><input type="text" name="productPrice" class="add_input" placeholder=" e.g., 13000">
              <div class="add_labels">Image URL</div><input type="text" name="url" class="add_input" placeholder=" e.g., http://imgur.com/fXd03F">
              <div class="add_labels">Description</div><textarea rows="4" cols="50" name="productDetail" class="add_input"></textarea>       
          </form>
          </center>
        </div>
      </div>
  </div>
